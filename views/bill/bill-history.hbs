<div class="container" style="margin-bottom: 50px;">
  <h6 id="request-message" style="text-align: center; font-weight: bold; font-size: 18px"></h6>
  <div id="content">
    <div class="row">
      <div class="col-lg-12">
        {{#if result}}
        <h2 style="text-align: center;">Order history</h2>
        <div class="table-responsive">
          <table class="table table-bordered">
            <thead class="thead-dark">
              <tr>
                <th>ID</th>
                <th>Product</th>
                <th>Booking date</th>
                <th>Total</th>
                <th>Status</th>
              </tr>
            </thead>
            <tbody>
              {{#each result}}
              <tr>
                <td>{{this._id}}</td>
                <td>{{this.books}}</td>
                <td>{{this.booking_date}}</td>
                <td>{{this.total_price}}</td>
                <td>{{this.status}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        {{else}}
        <h2>Order history is empty</h2>
        {{/if}}
      </div>
    </div>
  </div>
</div>

